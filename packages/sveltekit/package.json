{
  "name": "harry-supabase-sveltekit-auth-helpers-but-updated-deps",
  "version": "0.6.10",
  "description": "A collection of framework specific Auth utilities for working with Supabase.",
  "type": "module",
  "scripts": {
    "build": "pnpm package",
    "build:watch": "pnpm package:watch",
    "postinstall": "svelte-kit sync",
    "package": "svelte-kit sync && svelte-package",
    "package:watch": "svelte-package -w",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supabase/auth-helpers.git"
  },
  "keywords": [
    "Supabase",
    "Auth",
    "Svelte Kit",
    "Svelte"
  ],
  "author": "Supabase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supabase/auth-helpers/issues"
  },
  "homepage": "https://github.com/supabase/auth-helpers/tree/main/packages/sveltekit#readme",
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.463",
    "@sveltejs/package": "1.0.0-next.3",
    "del-cli": "^5.0.0",
    "fix-esm-import-path": "^1.3.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.9.0",
    "svelte2tsx": "^0.5.16",
    "tslib": "^2.3.1",
    "typescript": "^4.8.2",
    "vite": "3.1.0-beta.1"
  },
  "dependencies": {
    "@supabase/auth-helpers-shared": "^0.1.3",
    "@supabase/supabase-js": "^2.0.0-rc.8"
